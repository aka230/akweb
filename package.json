{
  "name": "akweb",
  "version": "1.0.5",
  "description": "akashic components for h5, web and miniapp",
  "main": "lib/index.js",
  "module": "es/index.js",
  "miniappConfig": {
    "main": "lib/miniapp/index",
    "main:wechat": "lib/wechat-miniprogram/index"
  },
  "files": [
    "build",
    "dist",
    "es",
    "lib"
  ],
  "keywords": [
    "Rax",
    "rax-component"
  ],
  "engines": {
    "npm": ">=3.0.0"
  },
  "scripts": {
    "dev": "build-scripts start",
    "start": "build-scripts start",
    "build": "build-scripts build",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "lint": "npm run eslint && npm run stylelint",
    "prepublishOnly": "npm run build",
    "publish": "npm run build && npm publish"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "rax-modal": "^1.6.5",
    "rax-text": "^2.0.0",
    "rax-view": "^2.0.0",
    "universal-env": "^3.0.0"
  },
  "peerDependencies": {
    "rax": "^1.1.0"
  },
  "devDependencies": {
    "@iceworks/spec": "^1.0.0",
    "@types/rax": "^1.0.0",
    "build-plugin-component": "^1.0.0",
    "build-scripts": "^1.1.1",
    "driver-universal": "^3.1.0",
    "eslint": "^7.0.0",
    "rax": "^1.1.0",
    "rax-test-renderer": "^1.0.0",
    "stylelint": "^13.7.2",
    "typescript": "4.6.4"
  },
  "componentConfig": {
    "name": "akweb",
    "title": "demo",
    "category": ""
  },
  "exports": {
    ".": {
      "default": "./es/index.js"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "typescript": "4.6.4"
  },
  "license": "MIT",
  "homepage": "https://unpkg.com/akweb@1.0.4/build/index.html"
}
